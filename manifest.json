{
    "manifest_version": 3,
    "name": "Mood Booster",
    "description": "A Mood Tracker and Mood Booster for Stress Relief.",
    "version": "1.0",
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
      }
    },
    "permissions": [
      "storage"
    ]
  }
  